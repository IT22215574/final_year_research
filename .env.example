# Mobile App Environment Variables (React Native/Expo)
# Copy this file to .env for development

# API Configuration
EXPO_PUBLIC_API_URL=http://localhost:5000
EXPO_PUBLIC_WS_URL=ws://localhost:5000
EXPO_PUBLIC_API_KEY=your-api-key-here

# Application Configuration
EXPO_PUBLIC_APP_NAME=Smart Fisher Lanka
EXPO_PUBLIC_APP_VERSION=1.0.0

# Authentication
EXPO_PUBLIC_JWT_EXPIRY=3600000

# Payment Configuration
EXPO_PUBLIC_PAYMENT_GATEWAY_URL=https://api.trackerplus.com
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key

# File Upload Configuration
EXPO_PUBLIC_MAX_FILE_SIZE=52428800
EXPO_PUBLIC_ALLOWED_FILE_TYPES=pdf,doc,docx,xls,xlsx,ppt,pptx,jpg,jpeg,png,gif

# Video Service Configuration
EXPO_PUBLIC_VIDEO_SERVICE_URL=http://localhost:8080
EXPO_PUBLIC_JITSI_DOMAIN=meet.jit.si

# Feature Flags
EXPO_PUBLIC_MARKETPLACE_ENABLED=true
EXPO_PUBLIC_FACE_RECOGNITION_ENABLED=true
EXPO_PUBLIC_EXAM_MONITORING_ENABLED=true
EXPO_PUBLIC_AI_MONITORING_ENABLED=true
EXPO_PUBLIC_NOTIFICATIONS_ENABLED=true

# Push Notifications (Firebase/OneSignal)
EXPO_PUBLIC_FIREBASE_API_KEY=
EXPO_PUBLIC_FIREBASE_PROJECT_ID=
EXPO_PUBLIC_ONESIGNAL_APP_ID=

# Analytics
EXPO_PUBLIC_ANALYTICS_ENABLED=false
EXPO_PUBLIC_GA_TRACKING_ID=

# Support Configuration
EXPO_PUBLIC_SUPPORT_EMAIL=""
EXPO_PUBLIC_SUPPORT_PHONE=+1-555-SmartFisherLanka
