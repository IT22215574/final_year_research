{
  "expo": {
    "name": "SMART FISHER LANKA",
    "slug": "faite",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/SFLLogo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Allow this app to access your photo library to upload images."
      }
    },
    "android": {
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/SFLLogo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.adwx2001.battiadds"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/SFLLogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/SFLLogo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "gradlePluginVersion": "8.3.1",
            "minSdkVersion": 24,
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "kotlinVersion": "1.9.25",
            "buildToolsVersion": "35.0.0"
          }
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "8ad0f96f-f73e-467b-a809-c37d49ce50d7"
      }
    },
    "owner": "HirushaRashmika",
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
